{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "userid",
        "id",
        "title",
        "body"
    ],
    "properties": {
        "userid": {
            "type": "integer",
            "default": 0,
            "title": "The userid Schema",
            "examples": [
                1
            ]
        },
        "id": {
            "type": "integer",
            "default": 0,
            "title": "The id Schema",
            "examples": [
                2
            ]
        },
        "title": {
            "type": "string",
            "default": "",
            "title": "The title Schema",
            "examples": [
                "automation"
            ]
        },
        "body": {
            "type": "string",
            "default": "",
            "title": "The body Schema",
            "examples": [
                "Hello"
            ]
        }
    },
    "examples": [{
        "userid": 1,
        "id": 2,
        "title": "automation",
        "body": "Hello"
    }]
}